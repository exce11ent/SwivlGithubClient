platform :ios, '10.0'
# Uncomment this line if you're using Swift
use_frameworks!
# ignore all warnings from all pods
inhibit_all_warnings!

def share_pods
	pod 'RxSwift', '~> 4.2'
	pod 'RxCocoa', '~> 4.2'
	pod 'Moya/RxSwift', '~> 11.0'
	
	pod 'Kingfisher', '~> 4.8'
	pod 'IGListKit', '~> 3.0'
end

target 'SwivlGithubClient' do
	share_pods
end